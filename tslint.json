{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "align": true,
    "array-type": {
      "options": "generic",
      "severity": "off"
    },
    "eofline": true,
    "class-name": true,
    "cyclomatic-complexity": true,
    "import-spacing": true,
    "indent": true,
    "member-access": false,
    "no-console": {
      "severity": "warning"
    },
    "no-debugger": {
      "severity": "warning"
    },
    "no-empty": false,
    "member-ordering": {
      "options": true,
      "severity": "error"
    },
    "quotemark": {
      "options": "single",
      "severity": "warning"
    },
    "prefer-const": false,
    "max-line-length": false,
    "forin": false,
    "no-unused-expression": false,
    "object-literal-key-quotes": {
      "options": "as-needed",
      "severity": "warning"
    },
    "object-literal-sort-keys": {
      "options": "ignore-case",
      "severity": "off"
    },
    "variable-name": {
      "options": [
        "allow-leading-underscore",
        "allow-trailing-underscore"
      ],
      "severity": "warning"
    },
    "no-string-literal": false
  },
  "rulesDirectory": []
}