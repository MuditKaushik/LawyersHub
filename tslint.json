{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {
    "align": true,
    "eofline": true,
    "class-name": true,
    "cyclomatic-complexity": true,
    "import-spacing": true,
    "indent": true,
    "member-access": false,
    "no-console": {
      "severity": "warning"
    },
    "no-debugger": {
      "severity": "warning"
    },
    "member-ordering": true
  },
  "rules": {
    "align": true,
    "eofline": true,
    "class-name": true,
    "cyclomatic-complexity": true,
    "import-spacing": true,
    "indent": true,
    "member-access": false,
    "no-console": {
      "severity": "warning"
    },
    "no-debugger": {
      "severity": "warning"
    },
    "no-empty": false,
    "member-ordering": true,
    "quotemark": {
      "options": "single",
      "severity": "warning"
    },
    "prefer-const": false,
    "max-line-length": true,
    "forin": false,
    "no-unused-expression": false,
    "variable-name": {
      "options": [
        "allow-leading-underscore",
        "allow-trailing-underscore"
      ],
      "severity": "warning"
    },
    "no-string-literal": false
  },
  "rulesDirectory": []
}